{
  "name": "proyecto_ionic",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:coverage": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "e2e:dev": "ng e2e --dev-server-target=",
    "lint": "ng lint",
    "android:dev": "ionic capacitor run android",
    "android:build": "ionic capacitor build android",
    "android:release": "ionic build --prod && ionic capacitor build android --prod --release",
    "keystore:generate": "node scripts/generate-keystore.js",
    "keystore:verify": "node scripts/verify-apk.js",
    "apk:verify": "node scripts/verify-apk.js",
    "apk:build": "node scripts/build-signed.js",
    "apk:debug": "node scripts/build-signed.js --debug",
    "apk:release": "node scripts/build-signed.js --release",
    "aab:generate": "node scripts/generate-aab.js",
    "aab:debug": "node scripts/generate-aab.js --debug",
    "aab:release": "node scripts/generate-aab.js --release",
    "aab:verify": "node scripts/verify-aab.js",
    "build:signed": "npm run apk:release",
    "build:debug": "npm run apk:debug",
    "build:aab": "npm run aab:release",
    "build:clean": "rm -rf www && rm -rf android/app/build",
    "android:setup": "npm run keystore:generate && npm run apk:release",
    "demo:signing": "node scripts/demo-signing.js",
    "env:check": "node scripts/check-environment.js",
    "env:test": "node scripts/multiplatform-test.js",
    "multiplatform:test": "npm run env:check && npm run env:test",
    "multiplatform:build": "npm run build && npm run android:build",
    "multiplatform:setup": "npm run env:check && npm run android:setup",
    "activity:build": "node scripts/activity-apk-build.js",
    "activity:zip": "node scripts/generate-activity-zip.js",
    "activity:complete": "npm run activity:build && npm run activity:zip",
    "playstore:docx": "node scripts/generate-playstore-docx.js",
    "playstore:complete": "npm run playstore:docx && echo 'Ficha de Play Store generada exitosamente'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.18",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/material": "^19.2.18",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@awesome-cordova-plugins/core": "^5.44.0",
    "@awesome-cordova-plugins/sqlite": "^5.46.0",
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "^7.2.0",
    "@capacitor/geolocation": "^7.1.2",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/ios": "7.2.0",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@capacitor/toast": "^7.0.1",
    "@ionic/angular": "^8.0.0",
    "@ionic/storage-angular": "^4.0.0",
    "animate.css": "^4.1.1",
    "cordova-sqlite-storage": "^7.0.0",
    "docx": "^9.5.1",
    "ionicons": "^7.0.0",
    "leaflet": "^1.9.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular-eslint/builder": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/schematics": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/cli": "^19.2.14",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/language-service": "^19.0.0",
    "@capacitor/cli": "^7.3.0",
    "@cypress/schematic": "^3.0.0",
    "@ionic/angular-toolkit": "^12.0.0",
    "@types/google.maps": "^3.58.1",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.19",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "archiver": "^7.0.1",
    "cypress": "latest",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "protractor": "~7.0.0",
    "typescript": "~5.6.3"
  },
  "description": "An Ionic project"
}
